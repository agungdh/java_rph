	<queryString>
		<![CDATA[SELECT m.*, k.nama, k.nik, tl.nama_t_tugas lama, tb.nama_t_tugas baru,
concat(lpad(day(m.tanggal), 2, 0), '-', lpad(month(m.tanggal), 2, 0), '-', year(m.tanggal)) tanggal_parsed
FROM mutasi m, karyawan k, tempat_tugas tl, tempat_tugas tb
WHERE m.id_karyawan = k.id
AND m.id_tempat_tugas_lama = tl.id
AND m.id_tempat_tugas_baru = tb.id
AND m.tanggal BETWEEN $P{awal} AND $P{akhir}]]>
	</queryString>
